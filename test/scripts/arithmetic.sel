var float_div = 4.0 / 2.0;

var float_mul_precedence = 3.0 + 1*2;
var float_div_precedence = 3.0 + 4/2;
var float_parenthesis = 4 / (1 + 1);

var negative = -1;
var positive = --1;

var string_add = "abc" + "123";
var string_mul = "abc" * 2;

print("float_add = ", float_add);
print("float_sub = ", float_sub);
print("float_mul = ", float_mul);
print("float_div = ", float_div);
print("float_mul_precedence = ", float_mul_precedence);
print("float_div_precedence = ", float_div_precedence);
print("float_parenthesis = ", float_parenthesis);
print("negative = ", negative);
print("positive = ", positive);
print("string_add = ", string_add);
